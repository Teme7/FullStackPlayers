<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <br>
        <h2>Stock Tracker</h2>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="stock-text">Enter Stock Symbol</label>
            <input type="text" id="stock-text" class="form-control" aria-describedby="stock-text" placeholder="Enter Stock To Track">
          </div>
          <div class="form-group">
            <label for="company-name">Company Name</label>
            <textarea class="form-control" id="company-name" aria-describedby="company-name" placeholder="Enter Company Name"></textarea>
          </div>
          <button id="submit" class="btn btn-primary float-right">Submit</button>
        </form>
        <h3>Stocks Added To Portfolio:</h3>
        <ul id="stock-list" class="list-group">
          {{#each stocks}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="stock/{{this.id}}">{{this.symbol}}</a>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
